{
    "name": "spatie/mailcoach-ui",
    "description": "The UI of the Mailcoach app",
    "keywords": [
        "spatie",
        "mailcoach-ui"
    ],
    "homepage": "https://mailcoach.app",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "aws/aws-sdk-php": "~3.0",
        "laravel/sanctum": "^2.6",
        "laravel/ui": "^3.0",
        "league/html-to-markdown": "^4.10|^5.0",
        "livewire/livewire": "^2.10",
        "spatie/laravel-collection-macros": "^7.11",
        "spatie/laravel-livewire-wizard": "^1.1.3",
        "spatie/laravel-mailcoach": "v6.x-dev as 5.5.8",
        "spatie/laravel-mailcoach-editor": "6.x-dev as 1.3.2",
        "spatie/laravel-mailcoach-mailgun-feedback": "6.x-dev as 4.0.1",
        "spatie/laravel-mailcoach-mailgun-setup": "6.x-dev as 4.0.1",
        "spatie/laravel-mailcoach-markdown-editor": "6.x-dev as 1.0",
        "spatie/laravel-mailcoach-monaco": "6.x-dev as 2.2",
        "spatie/laravel-mailcoach-postmark-feedback": "6.x-dev as 4.0.1",
        "spatie/laravel-mailcoach-postmark-setup": "6.x-dev as 4.0.1",
        "spatie/laravel-mailcoach-sendgrid-feedback": "6.x-dev as 4.0.1",
        "spatie/laravel-mailcoach-sendgrid-setup": "6.x-dev as 4.0.1",
        "spatie/laravel-mailcoach-ses-feedback": "6.x-dev as 4.0.1",
        "spatie/laravel-mailcoach-ses-setup": "6.x-dev as 4.0.1",
        "spatie/laravel-mailcoach-unlayer": "6.x-dev as 2.4",
        "spatie/laravel-welcome-notification": "^2.3",
        "symfony/sendgrid-mailer": "^6.1"
    },
    "suggest": {
        "laravel/horizon": "If you want to manage your queues using Horizon."
    },
    "require-dev": {
        "orchestra/testbench": "^7.5",
        "pestphp/pest": "^1.21",
        "phpunit/phpunit": "^9.5"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../mailcoach-packages/packages/*"
        },
        {
            "type": "path",
            "url": "../laravel-mailcoach"
        },
        {
            "type": "composer",
            "url": "https://satis.spatie.be"
        }
    ],
    "autoload": {
        "psr-4": {
            "Spatie\\MailcoachUi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\MailcoachUi\\Tests\\": "tests",
            "Spatie\\MailcoachUi\\Tests\\Database\\Factories\\": "tests/database/Factories"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\MailcoachUi\\MailcoachUiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
